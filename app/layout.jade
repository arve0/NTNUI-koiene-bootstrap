    script.
      var setMenuHeight = function(){
        var meny = $('.meny');
        if($(window).width() > 767) {
          meny.height($('.innhold').height());
        }
        else {
          meny.height('auto');
        }
      }
      var setLogoHeight = function(){
        var h = $('.top-img').height();
        $('.logo').height(h);
      }
      $(document).ready(function(){
        setInterval(setLogoHeight);
        setMenuHeight();
        $(window).resize(function(){
          setMenuHeight();
        });
      });
