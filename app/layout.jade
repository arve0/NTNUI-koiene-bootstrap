    script(type='text/javascript')
      function StatkartMapType() {
      this.tileSize = new google.maps.Size(256,256);
      this.maxZoom = 19;
      this.getTile = function(coord, zoom, ownerDocument) {
      var div = ownerDocument.createElement('DIV');
      div.style.width = this.tileSize.width + 'px';
      div.style.height = this.tileSize.height + 'px';
      div.style.backgroundImage = "url(http://opencache.statkart.no/gatekeeper/gk/gk.open_gmaps?layers=topo2&zoom=" + zoom + "&x=" + coord.x + "&y=" + coord.y + ")";
      return div;
      };
      }
      function initialize() {
      var mapOptions = {
      zoom: 14,
      center: new google.maps.LatLng(63.08642,11.64870),
      mapTypeId: google.maps.MapTypeId.TERRAIN,
      zoomControl: true,
      zoomControlOptions: {style: google.maps.ZoomControlStyle.SMALL},
      disableDefaultUI: false,
      scrollwheel: false,
      scaleControl: true,
      panControl: false,
      mapTypeControl: false,
      streetViewControl: false
      };
      var map = new google.maps.Map(document.getElementById('map_canvas'), mapOptions);
      var kmlLayer = new google.maps.KmlLayer("http://org.ntnu.no/koiene/koiene/maps/koiene.kml",{suppressInfoWindows: false, map: map, preserveViewport: true});
      kmlLayer.setMap(map);
      map.mapTypes.set('topo2',new StatkartMapType());
      map.setMapTypeId('topo2');
      }
    script(type='text/javascript', charset='UTF-8', src='./NTNUI Koiene_files/{common,map,util,onion,kml}.js')
